{
	"name": "account",
	"$schema": "../../../node_modules/nx/schemas/project-schema.json",
	"sourceRoot": "apps/account/app/src",
	"projectType": "application",
	"targets": {
		"build": {
			"executor": "@nx/webpack:webpack",
			"outputs": ["{options.outputPath}"],
			"defaultConfiguration": "production",
			"options": {
				"target": "node",
				"compiler": "tsc",
				"outputPath": "dist/apps/account/app",
				"main": "apps/account/app/src/main.ts",
				"tsConfig": "apps/account/app/tsconfig.app.json",
				"assets": ["apps/account/app/src/assets"],
				"webpackConfig": "apps/account/app/webpack.config.js"
			},
			"configurations": {
				"development": {},
				"production": {}
			}
		},
		"serve": {
			"executor": "@nx/js:node",
			"defaultConfiguration": "development",
			"options": {
				"buildTarget": "account:build",
				"runBuildTargetDependencies": true,
				"inspect": "inspect",
				"host": "localhost",
				"port": 9230
			},
			"configurations": {
				"development": {
					"buildTarget": "account:build:development"
				},
				"production": {
					"buildTarget": "account:build:production"
				}
			}
		},
		"lint": {
			"executor": "@nx/eslint:lint",
			"outputs": ["{options.outputFile}"],
			"options": {
				"lintFilePatterns": ["apps/account/app/**/*.ts"]
			}
		},
		"test": {
			"executor": "@nx/jest:jest",
			"outputs": ["{workspaceRoot}/coverage/{projectRoot}"],
			"options": {
				"jestConfig": "apps/account/app/jest.config.ts"
			}
		}
	},
	"tags": []
}
