{
	"name": "account-lib",
	"$schema": "../../../node_modules/nx/schemas/project-schema.json",
	"sourceRoot": "apps/account/lib/src",
	"projectType": "library",
	"targets": {
		"build": {
			"executor": "@nx/js:tsc",
			"outputs": ["{options.outputPath}"],
			"options": {
				"outputPath": "dist/apps/account/lib",
				"tsConfig": "apps/account/lib/tsconfig.lib.json",
				"packageJson": "apps/account/lib/package.json",
				"main": "apps/account/lib/src/index.ts",
				"assets": ["apps/account/lib/*.md"]
			}
		},
		"publish": {
			"command": "node tools/scripts/publish.mjs lib {args.ver} {args.tag}",
			"dependsOn": ["build"]
		},
		"lint": {
			"executor": "@nx/eslint:lint",
			"outputs": ["{options.outputFile}"],
			"options": {
				"lintFilePatterns": [
					"apps/account/lib/**/*.ts",
					"apps/account/lib/package.json"
				]
			}
		},
		"test": {
			"executor": "@nx/jest:jest",
			"outputs": ["{workspaceRoot}/coverage/{projectRoot}"],
			"options": {
				"jestConfig": "apps/account/lib/jest.config.ts"
			}
		}
	},
	"tags": []
}
