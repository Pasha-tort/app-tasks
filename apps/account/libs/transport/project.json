			}
		},
		"lint": {
			"executor": "@nx/eslint:lint",
			"outputs": ["{options.outputFile}"],
			"options": {
				"lintFilePatterns": ["apps/account/libs/transport/package.json"]
			}
		}
	},
	"tags": []
}
