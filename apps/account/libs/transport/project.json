	"$schema": "../../../../node_modules/nx/schemas/project-schema.json",
	"sourceRoot": "apps/account/libs/transport/src",
	"projectType": "library",
	"targets": {
		"build": {
			"executor": "@nx/js:tsc",
			"outputs": ["{options.outputPath}"],
			"options": {
				"outputPath": "dist/apps/account/libs/transport",
